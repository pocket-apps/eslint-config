{
  "name": "@pocket-studios/eslint-config",
  "version": "2.0.0",
  "main": "index.js",
  "repository": "git://github.com/pocket-studios/eslint-config.git",
  "author": "papeloto <vn4varro@gmail.com>",
  "license": "MIT",
  "publishConfig": {
    "registry": "https://npm.pkg.github.com/"
  },
  "scripts": {
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --check .",
    "format:fix": "prettier --write ."
  },
  "dependencies": {
    "@typescript-eslint/eslint-plugin": "3.10.1",
    "@typescript-eslint/parser": "3.10.1",
    "eslint": "7.2.0",
    "eslint-config-airbnb-base": "14.2.0",
    "eslint-config-prettier": "6.12.0",
    "eslint-import-resolver-typescript": "2.3.0",
    "eslint-plugin-cypress": "2.11.2",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-jest": "24.0.2",
    "eslint-plugin-jest-dom": "3.2.3",
    "eslint-plugin-jsx-a11y": "6.3.1",
    "eslint-plugin-react": "7.21.3",
    "eslint-plugin-react-hooks": "4.1.2",
    "eslint-plugin-replace-relative-imports": "1.0.0",
    "eslint-plugin-testing-library": "3.9.2",
    "eslint-plugin-unused-imports": "0.1.3"
  },
  "devDependencies": {
    "@pocket-studios/prettier-config": "1.0.0",
    "eslint": "7.2.0",
    "husky": "4.3.0",
    "lint-staged": "10.4.1",
    "prettier": "2.1.2",
    "typescript": "4.0.3"
  },
  "peerDependencies": {
    "eslint": "^6.8.0 || ^7.2.0",
    "prettier": "^2.0.0"
  },
  "eslintConfig": {
    "extends": "./index.js"
  },
  "prettier": "@pocket-studios/prettier-config",
  "husky": {
    "hooks": {
      "pre-push": "lint-staged"
    }
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "eslint"
    ],
    "*.+(js|json|yml|md)": [
      "prettier --write"
    ]
  }
}
